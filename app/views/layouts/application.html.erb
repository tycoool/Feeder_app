<!DOCTYPE html>
<html>
<head>
  <title>Feeder</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/holder/2.9.0/holder.min.js', 'data-turbolinks-track' => true %>

  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
  <%= csrf_meta_tags %>
</head>
<body>

  <%= render :partial=>"layouts/application_partials/navbar"%>
  <div class="container-fluid">
      <div class="row">
        <%= render :partial=>"layouts/application_partials/sidebar"%>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
          <%= render :partial=>"layouts/application_partials/notices"%>
            <%= yield %>
        </div>
      </div>
  </div>

</body>
</html>
